pipeline {
    agent any

    stages {
        stage('Git Checkout') {
            steps {
                git 'https://github.com/Pritam-Khergade/student-ui.git'
            }
        }
        stage('build stage') {
            steps {
                sh 'apt install maven -y'
                sh 'mvn clean install'
            }
        }


    }
}
