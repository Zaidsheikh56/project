pipeline {
    agent any

    stages {
        stage('Git Checkout') {
            steps {
                git 'https://github.com/Pritam-Khergade/student-ui.git'
            }
        }
        stage('build stage1') {
            steps {
                sh 'apt-cache update '
                sh 'apt-cache install maven -y'
                sh 'mvn clean install'
            }
        }


    }
}
